    bedeo:has_municipal_service_description "Full municipal services available"^^xsd:string ;
    bedeo:has_address bedeo:address_CurrieLot ;
    bedeo:has_zoning bedeo:zone_DC163D2016 .

# The address for the lot
bedeo:address_CurrieLot
    a bedeo:Address ;
    rdfs:label "Currie Lot Address" ;
    bedeo:has_string_representation "Approximately Phase 12B- Dieppe Avenue SW and Quesnay Wood Dr SW"^^xsd:string ;
